  color: #0F172B;
  text-decoration: none;
  font-weight: 500;
  font-size: 1rem;
  padding: 5px 10px;
  display: inline-flex;
  align-items: center;
  transition: all 0.3s ease;
}

.view-all-link:hover {
  color: #1a2542;
  text-decoration: underline;
}

.view-all-link i {
  transition: transform 0.3s ease;
}

